<view id="DoctorDetail"><view class="head"><image src="../../static/background.png" mode="aspectFill"></image><view class="padding-top-lg flex"><view class="padding-lr"><button data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="cu-btn cuIcon bg-grey text-xl" bindtap="__e"><text class="cuIcon-back"></text></button></view><view class="padding-lr" style="margin-left:auto;"><button class="cu-btn cuIcon margin-lr-xs bg-grey text-xl"><text class="cuIcon-favor"></text></button><button class="cu-btn cuIcon margin-lr-xs bg-grey text-xl"><text class="cuIcon-share"></text></button><button class="cu-btn cuIcon margin-lr-xs bg-grey text-xl"><text class="cuIcon-more"></text></button></view></view><view class="hospitalname margin-tb-lg margin-left"><view class="ShowHospitalName text-xxl"><text class="cuIcon-titles text-blue"></text><text class="text-black text-bold text-xxl" style="color:#000;">{{head[0].name}}</text><text class="margin-left text-df">{{head[1].name}}</text></view></view></view><view class="margin-lr radius padding-bottom-xl" style="height:auto;background-color:white;"><block wx:for="{{doctor}}" wx:for-item="item" wx:for-index="index"><view class="solids-bottom padding-bottom-xs margin-lr-sm text-center"><image class="cu-avatar xl round margin" id="avatar" src="{{item.avatar}}"></image><view><text class="text-xl text-cyan">{{item.name}}</text></view><view class="margin-tb-sm">{{item.level}}</view><view class="margin-tb-sm">{{item.introduce}}</view><view class="margin-tb-sm">{{item.skill}}</view></view></block><block wx:for="{{doctor}}" wx:for-item="item" wx:for-index="__i0__"><view class="margin-lr margin-tb-sm"><text class="text-xl flex justify-start" style="color:#000;">排班表</text><view class="margin-tb"><block wx:for="{{item.schedule}}" wx:for-item="items" wx:for-index="__i1__"><view class="cu-tag light bg-cyan radius" style="margin-left:20rpx;">{{items.name}}</view></block></view></view></block></view></view>